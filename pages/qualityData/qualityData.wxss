/* left-tab start */
.left_tabbar_item {
  color: #7c7c7c;
  font-size: 26rpx;
  width: 100%;
  padding: 20rpx 0;
  text-align: center;
  /* border-bottom: 1rpx solid #fff; */
}

.left_tabbar_list {
  background: #eee;
}

 .active {
  background: #fff;
  color: #000;
  transition: all 0.5s;
  animation: show 0.5s;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

@-webkit-keyframes show {
  0% {
    opacity: 0;
    transform: rotateY(180deg);
  }

  100% {
    opacity: 1;
    transform: rotateY(360deg);
  }
}

@keyframes show {
  0% {
    opacity: 0;
    transform: rotateY(180deg);
  }

  100% {
    opacity: 1;
    transform: rotateY(360deg);
  }
}

